    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "import nltk\n",
        "from tensorflow.keras.preprocessing.text import Tokenizer\n",
        "from tensorflow.keras.preprocessing.sequence import pad_sequences\n",
        "from tensorflow.keras.layers import Input, Embedding, LSTM, Dense, Concatenate, Dot, Activation, Lambda\n",
        "from tensorflow.keras.models import Model\n",
        "import tensorflow.keras.backend as K"
      ],
      "metadata": {
        "id": "F8HeSRLqIX1S"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "df = pd.read_csv(\"/content/drive/Shareddrives/data science/cnn_dailymail/train.csv\").dropna().sample(n=10000, random_state=42)\n",
        "df = df.rename(columns={'article': 'text', 'highlights': 'summary'})\n",
        "\n",
        "texts = df['text'].tolist()\n",
        "summaries = df['summary'].tolist()"
      ],
      "metadata": {
        "id": "tZ3dA-KhJLTl"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Tokenizers\n",
        "text_tokenizer = Tokenizer(oov_token='<OOV>')\n",
        "summary_tokenizer = Tokenizer(oov_token='<OOV>')\n",
        "\n",
        "text_tokenizer.fit_on_texts(texts)\n",
        "summary_tokenizer.fit_on_texts(summaries)\n",
        "\n",
        "# Vocabulary size\n",
        "text_vocab_size = len(text_tokenizer.word_index) + 1\n",
        "summary_vocab_size = len(summary_tokenizer.word_index) + 1\n",
        "\n",
        "# Sequence lengths\n",
        "max_text_len = 300\n",
        "max_summary_len = 50\n",
        "\n",
        "# Sequences\n",
        "encoder_input = pad_sequences(text_tokenizer.texts_to_sequences(texts), maxlen=max_text_len, padding='post')\n",
        "decoder_input = pad_sequences(summary_tokenizer.texts_to_sequences(summaries), maxlen=max_summary_len, padding='post')\n",
        "decoder_target = np.zeros_like(decoder_input)\n",
        "decoder_target[:, :-1] = decoder_input[:, 1:]\n"
      ],
      "metadata": {
        "id": "8XfqYfYqJO0B"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "embedding_dim = 100\n",
        "latent_dim = 256\n",
        "\n",
        "# Encoder\n",
        "encoder_inputs = Input(shape=(max_text_len,))\n",
        "enc_emb = Embedding(text_vocab_size, embedding_dim)(encoder_inputs)\n",
        "encoder_lstm, state_h, state_c = LSTM(latent_dim, return_sequences=True, return_state=True)(enc_emb)\n",
        "encoder_states = [state_h, state_c]\n",
        "\n",
        "# Decoder\n",
        "decoder_inputs = Input(shape=(None,))\n",
        "dec_emb = Embedding(summary_vocab_size, embedding_dim)(decoder_inputs)\n",
        "decoder_lstm = LSTM(latent_dim, return_sequences=True, return_state=True)\n",
        "decoder_outputs, _, _ = decoder_lstm(dec_emb, initial_state=encoder_states)\n",
        "\n",
        "# Attention\n",
        "attn_dot = Dot(axes=[2, 2])([decoder_outputs, encoder_lstm])\n",
        "attn_weights = Activation('softmax')(attn_dot)\n",
        "context_vector = Dot(axes=[2, 1])([attn_weights, encoder_lstm])\n",
        "\n",
        "# Combine context with decoder output\n",
        "decoder_combined_context = Concatenate(axis=-1)([context_vector, decoder_outputs])\n",
        "output_layer = Dense(summary_vocab_size, activation='softmax')\n",
        "decoder_final_output = output_layer(decoder_combined_context)\n",
        "\n",
        "# Model\n",
        "model = Model([encoder_inputs, decoder_inputs], decoder_final_output)\n",
        "model.compile(optimizer='adam', loss='sparse_categorical_crossentropy')"
      ],
      "metadata": {
        "id": "A2K5yJJiJsbE"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "model.fit(\n",
        "    [encoder_input, decoder_input],\n",
        "    np.expand_dims(decoder_target, -1),\n",
        "    batch_size=64,\n",
        "    epochs=50,\n",
        "    validation_split=0.1\n",
        ")\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
